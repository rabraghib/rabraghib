{
  "root": "content",
  "sourceRoot": "content/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "yarn ts-node --project tools/tsconfig.json -r tsconfig-paths/register -T tools/build-readme"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["content/**/*.ts"]
      }
    }
  },
  "tags": []
}
