{
  "root": "content",
  "sourceRoot": "content/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": ["node tools/build-ejs-templates.js -o {args.outputPath}"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["content/**/*.ts"]
      }
    }
  },
  "tags": []
}
